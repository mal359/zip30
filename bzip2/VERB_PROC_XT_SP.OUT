define syntax SHOW_LICENSE
   parameter P1, label=OPTION
      value (required)
   parameter P2, label=PRODUCT
      value (list)
   qualifier OUTPUT, default
      value (default="SYS$OUTPUT",type=$file)
   qualifier BRIEF, nonnegatable
   qualifier FULL, nonnegatable
   qualifier PRODUCER, nonnegatable
      value (required)
      placement=positional
   qualifier CHARGE_TABLE, nonnegatable
   qualifier UNIT_REQUIREMENTS, nonnegatable
   qualifier CLUSTER, nonnegatable
   qualifier USAGE, nonnegatable
   qualifier RELEASE_DATE, nonnegatable
      value (default="TODAY",type=$datetime)
   qualifier TERMINATION_DATE, nonnegatable
      value (default="TODAY",type=$datetime)
   qualifier BEFORE, nonnegatable
   qualifier SINCE, nonnegatable
   qualifier WARNING_INTERVAL, default
      value (default="30",type=$number)
   qualifier EXACT
   qualifier HIGHLIGHT, nonnegatable
      value (list,type=HIGHLIGHT_OPTIONS)
