define syntax SHOW_ACL
   image SETSHOSECUR
   parameter P1, label=OPTION
      value (required)
   parameter P2, label=INPUT, prompt="Object name"
      value (required)
   qualifier OBJECT_TYPE
      value (required,type=ACL_OBJECTS)
   qualifier CLASS
      value (required)
   qualifier BACKUP
   qualifier BEFORE
      value (default="TODAY")
   qualifier BY_OWNER
      value (type=$uic)
   qualifier CREATED
   qualifier EXCLUDE
      value (required,list)
   qualifier EXPIRED
   qualifier MODIFIED
   qualifier SINCE
      value (default="TODAY")
   qualifier STYLE, nonnegatable, default
      value (type=STYLE_OPTIONS)
   qualifier SYMLINK
      value (list,type=SYMLINK_OPTIONS)
   disallow (CLASS and OBJECT_TYPE)
   disallow ((BACKUP or CREATED or EXPIRED or MODIFIED) and not (
      BEFORE or SINCE))

