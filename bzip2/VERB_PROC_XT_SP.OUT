   disallow (neg MMS_DEPENDENCIES and (MMS_DEPENDENCIES.FILE or 
      MMS_DEPENDENCIES.SYSTEM_INCLUDE_FILES or neg MMS_DEPENDENCIES.SYSTEM_INCLUDE_FILES))
   disallow (neg EXCEPTIONS and (EXCEPTIONS.CLEANUP or neg EXCEPTIONS.CLEANUP or 
      EXCEPTIONS.EXPLICIT or EXCEPTIONS.IMPLICIT))
   disallow (EXCEPTIONS.EXPLICIT and EXCEPTIONS.IMPLICIT)
   disallow (ASSUME.WHOLE_PROGRAM and ASSUME.POINTERS_TO_GLOBALS)
   disallow any2 (TEMPLATE_DEFINE.ALL, TEMPLATE_DEFINE.LOCAL, 
      TEMPLATE_DEFINE.USED, TEMPLATE_DEFINE.ALL_REPOSITORY, TEMPLATE_DEFINE.USED_REPOSITORY, 
      TEMPLATE_DEFINE.IMPLICIT_LOCAL)
   disallow any2 (NAMES.UPPERCASE, NAMES.AS_IS)
   disallow (NAMES.TRUNCATED and NAMES.SHORTENED)
   disallow (BOTHCASE and NAMES.AS_IS)
   disallow (neg ANNOTATIONS and (ANNOTATIONS.ALL or ANNOTATIONS.NONE or 
      ANNOTATIONS.CODE or ANNOTATIONS.DETAIL or ANNOTATIONS.FEEDBACK or 
      ANNOTATIONS.INLINING or ANNOTATIONS.LOOP_TRANSFORMS or 
      ANNOTATIONS.LOOP_UNROLLING or ANNOTATIONS.PREFETCHING or 
      ANNOTATIONS.SHRINKWRAPPING or ANNOTATIONS.SOFTWARE_PIPELINING or 
      ANNOTATIONS.TAIL_CALLS))
   disallow (DEBUG.NONE and (DEBUG.ALL or DEBUG.TRACEBACK or 
      DEBUG.SYMBOLS))
   disallow (DEBUG.ALL and (DEBUG.NONE or neg DEBUG.TRACEBACK or neg 
      DEBUG.SYMBOLS))
   disallow (PREPROCESS_ONLY and (OBJECT or XREF or EXPORT_SYMBOLS))
   disallow (neg OBJECT and EXPORT_SYMBOLS)
   disallow any2 (EXPORT_SYMBOLS.ALL, EXPORT_SYMBOLS.AUTOMATIC, 
      EXPORT_SYMBOLS.EXPLICIT)
