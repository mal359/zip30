define verb ACCOUNTING
   image ACC
   parameter P1, label=INPUT
      value (list,default="SYS$MANAGER:.DAT;",type=$infile)
   qualifier BINARY
   qualifier BRIEF
   qualifier FULL
   qualifier LOG
   qualifier OUTPUT, default
      value (default="SYS$OUTPUT",type=$outfile)
   qualifier REPORT, default
      value (list,type=ACNT_REPORT_KEYWORDS)
   qualifier REJECTED
      value (required,type=$outfile)
   qualifier SUMMARY
      value (list,type=ACNT_SUMMARY_KEYWORDS)
   qualifier TITLE
      value (required)
   qualifier WIDE
   qualifier ACCOUNT
      value (required,list)
   qualifier ADDRESS
      value (required,list)
   qualifier BEFORE
      value (default="TODAY",type=$datetime)
   qualifier ENTRY
      value (required,list)
   qualifier IDENT
      value (required,list)
   qualifier IMAGE
      value (required,list)
   qualifier JOB
      value (required,list)
   qualifier NODE
      value (required,list)
   qualifier OWNER
      value (required,list)
   qualifier PRIORITY
      value (required,list)
   qualifier PROCESS
      value (required,list)
   qualifier QUEUE
      value (required,list)
   qualifier REMOTE_ID
      value (required,list)
   qualifier SINCE
      value (default="TODAY",type=$datetime)
   qualifier STATUS
      value (required,list)
   qualifier TERMINAL
      value (required,list)
   qualifier TYPE
      value (required,list)
   qualifier UIC
      value (required,list,type=$uic)
   qualifier USER
      value (required,list)
   qualifier SORT
      value (list,default="USER")
   qualifier PSI, syntax=PSI_ACCNTNG, nonnegatable
   qualifier X25, syntax=PSI_ACCNTNG, nonnegatable
   qualifier CSI, syntax=CSI_ACCNTNG
   disallow any2 (SUMMARY, SORT)
   disallow any2 (BRIEF, FULL, BINARY, SUMMARY)
   disallow (not SUMMARY and REPORT)

