      value (required,list)
   qualifier REPLACE
   qualifier MACRO
   qualifier OBJECT
   qualifier OUTPUT, default
      value (type=$outfile)
   qualifier NAMES
      value (type=NAMES_OPTIONS)
   qualifier FULL
   qualifier DEBUG, default
   qualifier GLOBALS, default
   qualifier RSX11, syntax=LIBRARY_USING_LBR
   qualifier SELECTIVE_SEARCH
   qualifier SQUEEZE, default
   qualifier LOG
   qualifier WIDTH, nonnegatable
      value (required)
   qualifier CROSS_REFERENCE, syntax=LIBRARY_ONLY
      value (list,type=CROSS_OPTIONS)
   qualifier ONLY
      value (required,list)
   qualifier HELP
   qualifier TEXT
   qualifier MODULES
      value (required,list)
      placement=local
   qualifier SHARE
   qualifier HISTORY
   qualifier BEFORE
      value (default="TODAY",type=$datetime)
   qualifier SINCE
      value (default="TODAY",type=$datetime)
   qualifier VAX, nonnegatable
   qualifier DEMANGLED_SYMBOLS, syntax=LIBRARY_ONLY
      value (type=$outfile)
   disallow (ALPHA and VAX)
   disallow (EXTRACT and (COMPRESS or CREATE or DELETE or INSERT or 
      LIST or REMOVE or REPLACE or NAMES or FULL or DEBUG or 
      GLOBALS or SELECTIVE_SEARCH or SQUEEZE or CROSS_REFERENCE or 
      WIDTH or DEMANGLED_SYMBOLS))
   disallow (COMPRESS and CREATE)
   disallow (CROSS_REFERENCE and (LIST or MACRO or HELP or TEXT or 
      BEFORE or SINCE or HISTORY or DEMANGLED_SYMBOLS))
