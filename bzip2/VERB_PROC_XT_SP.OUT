define verb AEST
   image AEST$MAIN
   parameter P1, label=INPUT_FILE, prompt="File"
      value (required,type=$infile)
   qualifier AUDIT
   qualifier INTERPRET
   qualifier LIST, default
      value (type=$outfile)
   qualifier DUMP
      value (list,type=DUMP_OPTIONS)
   qualifier EXECUTABLE, default
      value (type=$outfile)
   qualifier AIIF, nonnegatable
      value (required,list,type=$file)
   qualifier NOAIIF, nonnegatable
      value (required,list,type=$file)
   qualifier OPTIMIZE, default
      value (list,type=OPTIMIZE_OPTIONS)
   qualifier VERBOSE
   qualifier PV
   qualifier DEBUG
   disallow (DUMP.ALL and DUMP.NONE)
   disallow (DUMP.ALL and DUMP.ALPHA)
   disallow (DUMP.ALL and DUMP.IA64)
   disallow (DUMP.NONE and DUMP.ALPHA)
   disallow (DUMP.NONE and DUMP.IA64)
   disallow (DUMP.NONE and DUMP.INTERNAL)
   disallow (DUMP.ALPHA.ALL and DUMP.ALPHA.NONE)
   disallow (DUMP.ALPHA.VERBOSE and DUMP.ALPHA.NONE)
   disallow (OPTIMIZE.ALL and OPTIMIZE.NONE)

