      RELEASE_DATE or TERMINATION_DATE))
   disallow (UNIT_REQUIREMENTS and (FULL or BRIEF or PRODUCER or 
      PRODUCT or RELEASE_DATE or TERMINATION_DATE))
   disallow ((BEFORE or SINCE) and not (RELEASE_DATE or TERMINATION_DATE))
   disallow (FULL and BRIEF)
