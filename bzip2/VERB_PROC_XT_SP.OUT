define syntax UCM_HELP
   parameter P1, label=OPERATION, prompt="Operation"
      value (type=UCM_OPERATION)
   parameter P2, label=HELP_TEXT
      value (type=$rest_of_line)

define syntax UCM_SHOW
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SHOW_OPTIONS)

define type SHOW_OPTIONS
   keyword DEVICE, syntax=SHOW_DEVICE
   keyword EVENTS, syntax=SHOW_EVENTS
   keyword AUTO, syntax=SHOW_AUTO

define syntax SHOW_DEVICE
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SHOW_OPTIONS)
   parameter P3, label=DEVICE
   qualifier CONFIGURED, nonnegatable
   qualifier UNCONFIGURED, nonnegatable
   qualifier GENERIC, nonnegatable
   qualifier PERMANENT, nonnegatable
   qualifier PHYSICAL, nonnegatable
   qualifier ALL, nonnegatable, default
   qualifier BRIEF, nonnegatable
   qualifier FULL, nonnegatable
   disallow (BRIEF and FULL)

define syntax SHOW_EVENTS
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SHOW_OPTIONS)
   qualifier OUTPUT
      value (required,type=$file)
   qualifier TYPE
      value (list,default="ALL",type=TYPE_KEYWORDS)
   qualifier PRIORITY
      value (list,default="CRITICAL,NORMAL",type=PRIORITY_KEYWORDS)
   qualifier VALUE
      value (required,list,type=$number)
   qualifier SINCE
      value (default="TODAY",type=$datetime)
   qualifier BEFORE
      value (default="TODAY",type=$datetime)
   qualifier PAGE
   disallow (OUTPUT and PAGE)
   disallow (TYPE and VALUE)

define type TYPE_KEYWORDS
   keyword DECONFIGURED
   keyword CONFIGURED
   keyword UNCONFIGURED
   keyword UNKNOWN
   keyword UCM
   keyword DRIVER
   keyword ALL

define type PRIORITY_KEYWORDS
   keyword NORMAL
   keyword CRITICAL
   keyword INFORMATIONAL
   keyword DBG1
   keyword DBG2
   keyword DBG3
   keyword ALL

define syntax SHOW_AUTO
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SHOW_OPTIONS)
   noqualifiers

define syntax UCM_SET
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SET_OPTIONS)

define type SET_OPTIONS
   keyword LOG, syntax=SET_LOG
   keyword AUTO, syntax=SET_AUTO

define syntax SET_LOG
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SET_OPTIONS)
   qualifier NEW

define syntax SET_AUTO
   parameter P1, label=OPERATION, prompt="Operation"
      value (required,type=UCM_OPERATION)
   parameter P2, label=OPTION, prompt="What"
      value (required,type=SET_OPTIONS)
   qualifier EXCLUDE
      value (required,list,type=$device)
   qualifier INCLUDE
      value (required,list,type=$device)
   qualifier ENABLE
      value (list,default="LOAD,PERM",type=SET_KEYWORDS)
   qualifier DISABLE
      value (list,default="LOAD,PERM",type=SET_KEYWORDS)

define type SET_KEYWORDS
   keyword LOAD
   keyword PERM

define syntax UCM_RESET
   parameter P1, label=OPERATION, prompt="Operation"
      value (type=UCM_OPERATION)
   qualifier PATH, nonnegatable
      value (required)
   qualifier BUS_NUMBER, nonnegatable
      value (required,type=$number)
   qualifier ALL, nonnegatable
   qualifier UNCONFIGURED, nonnegatable
   disallow (PATH and ALL)
   disallow (UNCONFIGURED and PATH)
   disallow (UNCONFIGURED and ALL)
   disallow (UNCONFIGURED and BUS_NUMBER)

define verb UCX
   image TCPIP$UCP
   parameter P1
      value (type=$rest_of_line)
   noqualifiers

define verb UIL
   image DECW$UILMOTIF
   parameter P1, label=SOURCE_FILE, prompt="File"
      value (required,type=$infile)
   qualifier TRACE, label=TRACE_QUAL, nonnegatable
      value (list,noconcatenate,type=TRACE_KEYWORDS)
   qualifier WARNINGS, label=WARNINGS_QUAL
      value (list,noconcatenate,type=WARNING_KEYWORDS)
   qualifier LIST, label=LISTING_FILE, batch
      value (type=$outfile)
   qualifier MACHINE, label=MACHINE_QUAL
   qualifier OUTPUT, label=RESOURCE_FILE, default
      value (type=$outfile)
   qualifier MOTIF, syntax=MOTIF_UIL, nonnegatable, default
   qualifier WIDGET_META_DESCRIPTION, label=WIDGET_QUAL
      value (required,type=$infile)
   qualifier LOCALE, nonnegatable

define type TRACE_KEYWORDS
   keyword TOKENS
   keyword SYMBOLS

define type WARNING_KEYWORDS
   keyword NOWARNINGS
   keyword NOINFORMATIONALS

define syntax MOTIF_UIL
   image DECW$UILMOTIF

define verb UNLOCK
   image UNLOCK
   parameter P1, prompt="File"
      value (required,list,impcat,type=$infile)
   qualifier CONFIRM
   qualifier LOG

define verb VIEW
   image DDIF$VIEW
   parameter P1, label=DVR$INPUT
      value (type=$infile)
   qualifier FORMAT, label=DVR$FORMAT, nonnegatable
      value (default="DDIF",type=$quoted_string)
      placement=positional
   qualifier OPTIONS, label=DVR$OPTIONS
      value (required,type=$infile)
      placement=positional
   qualifier OUTPUT, label=DVR$OUTPUT, batch
      value (type=$outfile)
      placement=positional
   qualifier PAGE, label=DVR$PAGE
      placement=positional
   qualifier SELECT, label=DVR$SELECT, nonnegatable
      value (list,type=SELECT_LIST_OPTIONS)
      placement=positional
   qualifier INTERFACE, label=DVR$INTERFACE, nonnegatable, default
      value (default="character_cell",type=DISPLAY_KEYWORDS)
      placement=positional
   qualifier OVERRIDE_FORMAT, label=DVR$OVERRIDE_FORMAT
   qualifier WIDTH, label=DVR$WIDTH, nonnegatable
      value (required,type=$number)
   qualifier HEIGHT, label=DVR$HEIGHT, nonnegatable
      value (required,type=$number)
   disallow ((DVR$SELECT.X_DISPLAY or DVR$INTERFACE.DECWINDOWS) and 
      DVR$OUTPUT)
   disallow ((DVR$SELECT.X_DISPLAY or DVR$INTERFACE.DECWINDOWS) and 
      DVR$PAGE)
   disallow (DVR$PAGE and DVR$OUTPUT)

define type SELECT_LIST_OPTIONS
   keyword ALL
   keyword AUTO_WRAP, negatable
   keyword GRAPHICS, negatable, default
   keyword IMAGES, negatable, default
   keyword SOFT_DIRECTIVES, negatable
   keyword TEXT, negatable, default
   keyword X_DISPLAY, negatable

define type DISPLAY_KEYWORDS
   keyword DECWINDOWS
   keyword CHARACTER_CELL

define verb WAIT
   cliroutine WAIT, cliflags(nostatus, immediate)

define verb WRITE
   cliroutine WRITE
   prefix CLI$K_WRIT_
   parameter P1, prompt="Log name"
      value (required,type=$inlog)
   parameter P2, prompt="Expression"
      value (required,list,type=$expression)
   qualifier END_OF_FILE
      value (required)
   qualifier ERROR
      value (required)
   qualifier UPDATE
   qualifier SYMBOLS, syntax=WRITE_SYMBOL_OUT, nonnegatable
   qualifier WAIT
   disallow (WAIT and (UPDATE or SYMBOLS))

define syntax WRITE_SYMBOL_OUT
   parameter P1, prompt="Log name"
      value (required,type=$inlog)
   parameter P2, prompt="Symbol"
      value (required,list,type=$quoted_string)

define verb XAUTH
   image DECW$XAUTH
   parameter P1, label=OPTION
      value (type=$rest_of_line)

define verb XPROXYMANAGER
   image DECW$PROXYMANAGER
   qualifier CONFIGURATION, nonnegatable
      value (required,type=$file)
   qualifier LOG
      value (type=$file)
   qualifier VERBOSE
   qualifier PORT, nonnegatable
      value (required,type=$number)
   qualifier TRANSPORT, nonnegatable
      value (required,list)

define verb ZIC
   image ZIC
   parameter P1, label=OPTION1
      value (type=$file)
   parameter P2, label=OPTION2
      value (type=$file)
   parameter P3, label=OPTION3
      value (type=$file)
   parameter P4, label=OPTION4
      value (type=$file)
   parameter P5, label=OPTION5
      value (type=$file)
   parameter P6, label=OPTION6
      value (type=$file)
   parameter P7, label=OPTION7
      value (type=$file)
   parameter P8, label=OPTION8
      value (type=$file)
   nodisallows

