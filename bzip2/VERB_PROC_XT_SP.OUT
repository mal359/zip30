define verb HELP
   image VMSHELP
   parameter P1, label=KEYWORDS
      value (type=$rest_of_line)
   qualifier PROMPT, default
   qualifier INSTRUCTIONS, default
   qualifier OUTPUT, default
      value (default="SYS$OUTPUT",type=$file)
   qualifier LIBRARY, default
      value (default="HELPLIB",type=$infile)
   qualifier LIBLIST, default
   qualifier USERLIBRARY, default
      value (list,type=SCOPE_KEYWORDS)
   qualifier EXACT
   qualifier HIGHLIGHT, nonnegatable
      value (list,type=HIGHLIGHT_OPTIONS)
   qualifier PAGE, default
      value (list,type=PAGE_OPTIONS)
   qualifier SEARCH, nonnegatable
      value (required)
   qualifier WRAP
   qualifier MESSAGE, syntax=MSGHLP
   disallow (OUTPUT and PAGE.PAGE_SAVE)
   disallow (not PAGE.PAGE_SAVE and (EXACT or SEARCH or WRAP or 
      HIGHLIGHT))
   disallow any2 (HIGHLIGHT.HIGHLIGHT_BLUE, HIGHLIGHT.HIGHLIGHT_CYAN, 
      HIGHLIGHT.HIGHLIGHT_GREEN, HIGHLIGHT.HIGHLIGHT_MAGENTA, 
      HIGHLIGHT.HIGHLIGHT_RED, HIGHLIGHT.HIGHLIGHT_YELLOW)
   disallow (USERLIBRARY.ALL and USERLIBRARY.NONE)
   disallow ((USERLIBRARY.ALL or USERLIBRARY.NONE) and (USERLIBRARY.PROCESS or 
      USERLIBRARY.GROUP or USERLIBRARY.SYSTEM))

