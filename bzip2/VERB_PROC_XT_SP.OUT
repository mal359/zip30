define syntax MSGHLP
   image MSGHLP$MAIN
   parameter P1
      value (list)
   parameter P2
      value (list)
   parameter P3
      value (list)
   parameter P4
      value (list)
   parameter P5
      value (list)
   parameter P6
      value (list)
   parameter P7
      value (list)
   parameter P8
      value (list)
   qualifier LIBRARY, label=MSGHLP$K_LIBRARY, nonnegatable
      value (default="MSGHLP$LIBRARY",type=$file)
   qualifier INSERT, syntax=MSGHLP_UPDATE, label=MSGHLP$K_INSERT
      value (required,type=$file)
   qualifier DELETE, syntax=MSGHLP_UPDATE, label=MSGHLP$K_DELETE
      value (required,type=$file)
   qualifier STATUS, label=MSGHLP$K_STATUS, nonnegatable
      value (required,type=$number)
   qualifier FACILITY, label=MSGHLP$K_FACILITY, nonnegatable
      value (list,default="?")
   qualifier EXTRACT, label=MSGHLP$K_EXTRACT, nonnegatable
      value (required,type=$file)
   qualifier OUTPUT, label=MSGHLP$K_OUTPUT
      value (required,type=$file)
   qualifier PAGE, label=MSGHLP$K_PAGE, default
   qualifier SORT, label=MSGHLP$K_SORT
   qualifier WORK_FILES, label=MSGHLP$K_WORK_FILES
      value (type=$number)
   qualifier FULL, label=MSGHLP$K_FULL, nonnegatable
   qualifier BRIEF, label=MSGHLP$K_BRIEF, nonnegatable
   qualifier WORD_MATCH, label=MSGHLP$K_WORD_MATCH, nonnegatable
      value (required,type=MSGHLP_MATCH)
   qualifier SECTION_FILE, label=MSGHLP$K_FILE_NAME, nonnegatable
      value (required,type=$file)
   qualifier FILE_NAME, label=MSGHLP$K_LIB_NAME, nonnegatable
      value (required,type=$file)
   disallow (MSGHLP$K_INSERT and MSGHLP$K_DELETE)
   disallow (P1 and (MSGHLP$K_INSERT or MSGHLP$K_DELETE))
   disallow (MSGHLP$K_STATUS and P1)
   disallow (MSGHLP$K_STATUS and MSGHLP$K_FACILITY)
   disallow (MSGHLP$K_FULL and MSGHLP$K_BRIEF)
   disallow ((MSGHLP$K_BRIEF or MSGHLP$K_OUTPUT or MSGHLP$K_PAGE) and 
      MSGHLP$K_EXTRACT)
   disallow ((MSGHLP$K_STATUS or MSGHLP$K_FACILITY or MSGHLP$K_EXTRACT or 
      MSGHLP$K_OUTPUT or MSGHLP$K_PAGE or MSGHLP$K_SORT or MSGHLP$K_BRIEF or 
      MSGHLP$K_WORD_MATCH.MSGHLP$K_NOSUBSTRING) and (MSGHLP$K_INSERT or 
      MSGHLP$K_DELETE))
   disallow MSGHLP$K_LIB_NAME
   disallow (MSGHLP$K_FILE_NAME and MSGHLP$K_LIB_NAME)
   disallow ((MSGHLP$K_BRIEF or MSGHLP$K_OUTPUT or MSGHLP$K_PAGE or 
      MSGHLP$K_EXTRACT) and (MSGHLP$K_FILE_NAME or MSGHLP$K_LIB_NAME))
   disallow ((MSGHLP$K_STATUS or MSGHLP$K_FACILITY or MSGHLP$K_EXTRACT or 
      MSGHLP$K_OUTPUT or MSGHLP$K_PAGE or MSGHLP$K_SORT or MSGHLP$K_BRIEF or 
      MSGHLP$K_WORD_MATCH.MSGHLP$K_NOSUBSTRING or MSGHLP$K_INSERT or 
      MSGHLP$K_DELETE) and (MSGHLP$K_FILE_NAME or MSGHLP$K_LIB_NAME))

