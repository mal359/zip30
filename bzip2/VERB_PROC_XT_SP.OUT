define syntax SET_SERVER
   image SETSHOSERVER
   parameter P1, label=OPTION
      value (required)
   parameter P2, label=NAME, prompt="Name"
      value (required,type=SERVER_NAME)
   qualifier START, nonnegatable
      value (list,type=SET_SERVER_START)
   qualifier RESTART, nonnegatable
   qualifier EXIT, nonnegatable
   qualifier ABORT, nonnegatable
   qualifier CANCEL, nonnegatable
   qualifier SUSPEND, nonnegatable
   qualifier RESUME, nonnegatable
   qualifier DUMP, default
   qualifier WAIT
   qualifier CONFIGURE, nonnegatable
      value (list,type=SET_SERVER_CONFIGURE)
   qualifier ENABLE, nonnegatable
      value (type=SET_SERVER_ENABLE)
   qualifier DISABLE, nonnegatable
   qualifier NODE
      value (list)
   qualifier CLUSTER
   qualifier MASTER
   qualifier TRACE
      value (type=$number)
   qualifier LOG
      value (type=$file)
   qualifier UIC, nonnegatable
      value (required,type=$uic)
   qualifier PRIVILEGES, nonnegatable
      value (required,list,type=PRIVILEGES)
   qualifier AST_LIMIT, nonnegatable
      value (required,type=$number)
   qualifier BUFFER_LIMIT, nonnegatable
      value (required,type=$number)
   qualifier ENQUEUE_LIMIT, nonnegatable
      value (required,type=$number)
   qualifier EXTENT, nonnegatable
      value (required,type=$number)
   qualifier FILE_LIMIT, nonnegatable
      value (required,type=$number)
   qualifier IO_BUFFERED, nonnegatable
      value (required,type=$number)
   qualifier IO_DIRECT, nonnegatable
      value (required,type=$number)
   qualifier JOB_TABLE_QUOTA, nonnegatable
      value (required,type=$number)
   qualifier MAXIMUM_WORKING_SET, nonnegatable
      value (required,type=$number)
   qualifier PAGE_FILE, nonnegatable
      value (required,type=$number)
   qualifier QUEUE_LIMIT, nonnegatable
      value (required,type=$number)
   qualifier SUBPROCESS_LIMIT, nonnegatable
      value (required,type=$number)
   qualifier TIME_LIMIT, nonnegatable
      value (required,type=$deltatime)
   qualifier WORKING_SET, nonnegatable
      value (required,type=$number)
   qualifier QUOTAS, nonnegatable
      value (required,list,type=QUOTAS)
   disallow (MASTER and START)
   disallow any2 (NODE, CLUSTER, MASTER)
   disallow any2 (START, RESTART, EXIT, ABORT, SUSPEND, RESUME)
   disallow (DUMP and (RESTART or EXIT or ABORT or SUSPEND or 
      RESUME))
   disallow ((EXIT or ABORT or SUSPEND or RESUME) and (CONFIGURE or 
      ENABLE or DISABLE or TRACE or LOG))
   disallow ((WAIT or neg WAIT) and not (EXIT or DISABLE or SUSPEND))
   disallow (CANCEL and neg WAIT)
   disallow (CANCEL and (START or RESTART or ABORT or RESUME or 
      CONFIGURE or ENABLE or TRACE or LOG or SUSPEND))
   disallow (DISABLE and ENABLE)
   disallow (START.AUTO and not NAME.ACME_SERVER)
   disallow (NAME.ACME_SERVER and MASTER)
   disallow (NAME.ACME_SERVER and TIME_LIMIT)
   disallow (NAME.ACME_SERVER and QUOTAS.CPULM)
   disallow (not CONFIGURE.NAME and (CONFIGURE.FACILITY or CONFIGURE.FILE or 
      CONFIGURE.CREDENTIALS))
   disallow (NAME.REGISTRY_SERVER and (CONFIGURE or ENABLE or 
      DISABLE))
   disallow (NAME.REGISTRY_SERVER and (SUSPEND or RESUME or CANCEL))
   disallow (NAME.REGISTRY_SERVER and (SUBPROCESS_LIMIT or TIME_LIMIT))
   disallow (NAME.REGISTRY_SERVER and (QUOTAS.PRCLM or QUOTAS.CPULM))
   disallow (NAME.SECURITY_SERVER and (ABORT or CONFIGURE or 
      ENABLE or DISABLE or TRACE or LOG or MASTER))
   disallow (NAME.SECURITY_SERVER and (SUSPEND or RESUME or CANCEL))
   disallow (NAME.SECURITY_SERVER and (SUBPROCESS_LIMIT or TIME_LIMIT))
   disallow (NAME.SECURITY_SERVER and (QUOTAS.PRCLM or QUOTAS.CPULM))
   disallow (not START and (UIC or PRIVILEGES))
   disallow (not START and (AST_LIMIT or BUFFER_LIMIT or ENQUEUE_LIMIT or 
      EXTENT or FILE_LIMIT or IO_BUFFERED or IO_DIRECT or JOB_TABLE_QUOTA or 
      MAXIMUM_WORKING_SET or PAGE_FILE or QUEUE_LIMIT or SUBPROCESS_LIMIT or 
      TIME_LIMIT or WORKING_SET))
   disallow (not START and QUOTAS)
   disallow ((PRIVILEGES.ALL or neg PRIVILEGES.ALL) and (PRIVILEGES.SAME or 
      neg PRIVILEGES.SAME))

