define syntax SET_SECURITY
   image SETSHOSECUR
   parameter P1, label=OPTION
      value (required)
   parameter P2, label=INPUT, prompt="Object"
      value (required,type=$infile)
   qualifier ACL
      value (list,type=$acl)
   qualifier AFTER
      value (required,type=$acl)
   qualifier BACKUP
   qualifier BEFORE
      value (default="TODAY")
   qualifier BY_OWNER
      value (type=$uic)
   qualifier CONFIRM
   qualifier COPY_ATTRIBUTE
      value (required,list,type=SECUR_COPY_ATTR)
   qualifier CREATED
   qualifier DEFAULT
   qualifier DELETE
      value (type=SET_SECUR_DELETE)
   qualifier EDIT, syntax=ACLEDIT, nonnegatable
   qualifier EXCLUDE
      value (required,list)
   qualifier EXPIRED
   qualifier LIKE
      value (required,list,type=SET_SECUR_LIKE)
   qualifier LOG
   qualifier MODIFIED
   qualifier OBJECT_TYPE
      value (required)
   qualifier CLASS
      value (required)
   qualifier OWNER
      value (type=$uic)
   qualifier PROFILE
      value (required,type=SECUR_PROFILE)
   qualifier PROTECTION
      value (required,list,type=PROTECTION)
   qualifier REPLACE
      value (required,list,type=$acl)
   qualifier SINCE
      value (default="TODAY")
   qualifier SECRECY, label=SECURITY
      value (required,type=$acl)
   qualifier SQ1
      value (required)
   qualifier SQ2
      value (required)
   qualifier SQ3
      value (required)
   qualifier SQ4
      value (required)
   qualifier STYLE, nonnegatable, default
      value (type=STYLE_OPTIONS)
   qualifier SYMLINK
      value (list,type=SYMLINK_OPTIONS_NOTARG)
   qualifier INTEGRITY
      value (required,type=$acl)
   disallow (CLASS and OBJECT_TYPE)
   disallow (EDIT and (OWNER or PROTECTION or LIKE or DEFAULT))
   disallow (LIKE and DEFAULT)
   disallow ((LIKE or DEFAULT) and (OWNER or PROTECTION or ACL))
   disallow (COPY_ATTRIBUTE and not (DEFAULT or LIKE))
   disallow any2 (DELETE, AFTER, REPLACE)
   disallow ((REPLACE or AFTER or DELETE) and not ACL)
   disallow any2 (BACKUP, CREATED, EXPIRED, MODIFIED)
   disallow ((BACKUP or CREATED or EXPIRED or MODIFIED) and not (
      BEFORE or SINCE))

