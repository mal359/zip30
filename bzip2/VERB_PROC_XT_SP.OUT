      value (required,list,type=TEMPLATE_OPTIONS)
      placement=positional
   qualifier UNDEFINE
      value (list,type=$quoted_string)
      placement=positional
   qualifier UNSIGNED_CHAR
      placement=positional
   qualifier WARNINGS, default
      value (list,type=WARNING_OPTIONS)
      placement=positional
   qualifier XREF
      value (type=$outfile)
      placement=positional
   disallow (G_FLOAT and FLOAT.D_FLOAT)
   disallow (G_FLOAT and FLOAT.IEEE_FLOAT)
   disallow (G_FLOAT and IEEE_MODE)
   disallow (G_FLOAT and ROUNDING_MODE)
   disallow (FLOAT.G_FLOAT and IEEE_MODE)
   disallow (FLOAT.G_FLOAT and ROUNDING_MODE)
   disallow (FLOAT.D_FLOAT and IEEE_MODE)
   disallow (FLOAT.D_FLOAT and ROUNDING_MODE)
   disallow (PREFIX_LIBRARY_ENTRIES.ALL_ENTRIES and PREFIX_LIBRARY_ENTRIES.ANSI_C89_ENTRIES)
