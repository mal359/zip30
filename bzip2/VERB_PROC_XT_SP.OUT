define verb ALLOCATE
   cliroutine ALLOCATE
   prefix CLI$K_ALLO_
   parameter P1, prompt="Device"
      value (required,list,type=$device)
   parameter P2, prompt="Log name"
      value (type=$outlog)
   qualifier GENERIC
   qualifier LOG

define verb ANALYZE
   image ANALYZOBJ
   parameter P1, label=FILE_SPECS, prompt="File(s)"
      value (required,list,type=$infile)
   qualifier AUDIT, syntax=ANALYZE_AUDIT, nonnegatable
   qualifier CRASH_DUMP, syntax=ANALYZE_CRASH_DUMP, nonnegatable
   qualifier DISASSEMBLE, nonnegatable
   qualifier DISK_STRUCTURE, syntax=ANALYZE_DISK_STRUCTURE, nonnegatable
   qualifier ERROR_LOG, syntax=ANALYZE_ERROR_LOG_ERF, nonnegatable
   qualifier EXECUTABLE, syntax=ANALYZE_IMAGE, nonnegatable
   qualifier IMAGE, syntax=ANALYZE_IMAGE, nonnegatable
   qualifier MEDIA, syntax=ANALYZE_MEDIA, nonnegatable
   qualifier OBJECT, nonnegatable, default
   qualifier RMS_FILE, syntax=ANALYZE_RMS_FILE, nonnegatable
   qualifier SHARED_MEMORY, syntax=ANALYZE_SHARED_MEMORY, nonnegatable
   qualifier SSLOG, syntax=ANALYZE_SSLOG, nonnegatable
   qualifier SYSTEM, syntax=ANALYZE_SYSTEM, nonnegatable
   qualifier PROCESS_DUMP, syntax=ANALYZE_PROCESS_DUMP, nonnegatable
   qualifier ALPHA, nonnegatable
   qualifier DBG
      placement=positional
   qualifier EOM
      placement=positional
   qualifier GSD
      placement=positional
   qualifier INCLUDE
      value (list,default="*")
      placement=local
   qualifier INTERACTIVE
   qualifier LNK
      placement=positional
   qualifier MHD
      placement=positional
   qualifier OUTPUT, default
      value (default="SYS$OUTPUT",type=$outfile)
   qualifier TBT
      placement=positional
   qualifier TIR
      placement=positional
   qualifier SELECT
      value (required,list,type=SELECT_OPTIONS)
   qualifier HEADER
   qualifier SEGMENTS, nonnegatable
      value (list,default="HEADERS",type=SEGMENTS_OPTIONS)
      placement=positional
   qualifier SECTIONS, nonnegatable
      value (list,default="HEADERS",type=SECTIONS_OPTIONS)
      placement=positional
   qualifier FLAGVALUES, nonnegatable
      value (default="ALL",type=FLAGVAL_OPTIONS)
      placement=positional
   qualifier PAGE_BREAK
      value (required,type=PAGE_BREAK_OPTION)
   qualifier MODULE, nonnegatable
      value (list)
   disallow MODULE
   disallow (INCLUDE and SELECT)
   disallow (DISASSEMBLE and (ALPHA or DBG or EOM or GSD or INTERACTIVE or 
      LNK or MHD or TBT or TIR or SELECT or SEGMENTS or SECTIONS or 
      FLAGVALUES))
   disallow (SECTIONS.TRACE.FRAME or SECTIONS.TRACE.PUBNAMES or 
      SECTIONS.TRACE.PUBTYPES)
   disallow SELECT.VERSION_NUMBERS

define syntax ANALYZE_AUDIT
   image ANALAUDIT
   parameter P1, label=FILE_SPECS, prompt="File(s)"
      value (list,type=$infile)
   qualifier BEFORE
      value (default="TODAY",type=$datetime)
   qualifier BINARY
   qualifier BRIEF
   qualifier BY_OWNER
      value (type=$uic)
   qualifier EVENT_TYPE, default
      value (list,default="ALL",type=AUDIT_KEYWORDS)
   qualifier EXCLUDE
      value (required,list)
   qualifier FULL
   qualifier IGNORE
      value (required,list,type=AUDIT_SELECT_KEYWORDS)
   qualifier INTERACTIVE, default
   qualifier OUTPUT
      value (type=$outfile)
   qualifier PACKET_TYPE
   qualifier PAUSE
      value (type=$number)
   qualifier REMOVE
   qualifier SELECT
      value (required,list,type=AUDIT_SELECT_KEYWORDS)
   qualifier SINCE
      value (default="TODAY",type=$datetime)
   qualifier SUMMARY
      value (list,type=AUDIT_SUMMARY_KEYWORDS)
   qualifier SYMLINK
      value (required,type=SYMLINK_OPTIONS)
   disallow (FULL and BRIEF)
   disallow (FULL and BINARY)
   disallow (BRIEF and BINARY)

