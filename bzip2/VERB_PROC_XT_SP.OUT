   qualifier LIST
      value (type=$outfile)
   qualifier OUTPUT, default
      value (type=$outfile)
   qualifier READ_CHECK
   qualifier REPAIR
   qualifier USAGE
      value (type=$outfile)
   qualifier LOCK_VOLUME
   qualifier TRACE
   qualifier RECORD_ATTRIBUTES
   qualifier HOMEBLOCKS
   qualifier STATISTICS
   qualifier PROGRESS
      value (type=$number)
   qualifier SHADOW, syntax=ANALYZE_SHADOW_MEMBERS, nonnegatable

