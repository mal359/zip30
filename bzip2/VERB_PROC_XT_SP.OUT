   disallow (WARNING_INTERVAL and (CHARGE_TABLE or UNIT_REQUIREMENTS))
   disallow ((OE or HIERARCHY) and (CHARGE_TABLE or UNIT_REQUIREMENTS or 
      WARNING_INTERVAL))
