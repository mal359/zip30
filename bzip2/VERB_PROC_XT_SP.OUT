define syntax ANALYZE_CRASH_DUMP
   image SDA
   parameter P1, label=DUMP_FILE
      value (list,type=$infile)
   qualifier COLLECTION
      value (required,type=$infile)
   qualifier CRASH_DUMP, default
   qualifier LOG
   qualifier OVERRIDE
   qualifier RELEASE
   qualifier SHADOW_MEMBER, nonnegatable
      value (type=$infile)
   qualifier SHARED_MEMORY
   qualifier SYMBOLS
      value (required,type=$infile)
   qualifier SYSTEM
   disallow SHARED_MEMORY
   disallow SYSTEM
   disallow (COLLECTION and RELEASE)
   disallow (COLLECTION and OVERRIDE)
   disallow (OVERRIDE and RELEASE)
   disallow (SHADOW_MEMBER and RELEASE)

define syntax ANALYZE_DISK_STRUCTURE
